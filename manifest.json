{
  "manifest_version": 3,
  "name": "changeURL",
  "version": "1.0.0",
  "description": "An extension to change url after loading some specific pages",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.reddit.com/*", "https://example.com/*"],
      "js": ["Content.js"]
    }
  ]
}
